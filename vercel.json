{
  "version": 2,
  "builds": [{
    "src": "app.js",
    "use": "@vercel/node"
  }],
  "routes": [{
    "src": "/(.*)",
    "dest": "app.js"
  }],
  "functions": {
    "api/index.js": {
      "runtime": "@vercel/node@2.7.0"
    }
  },
  "rewrites": [{ "source": "/api/(.*)", "destination": "/api" }]
}
